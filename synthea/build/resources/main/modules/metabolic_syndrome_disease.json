      "attribute": "diabetic_nerve_damage",
      "value": 2,
      "direct_transition": "Moderate_Nerve_Damage_Symptom"
    },
    "Moderate_Nerve_Damage_Symptom": {
      "type": "Symptom",
      "symptom": "Tingling in Hands and Feet",
      "range": {
        "low": 20,
